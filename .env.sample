# The environment in which the application is running (e.g., dev, prod)
IMAGEFLOW_ENV=your_environment

# The port number the application will listen on
IMAGEFLOW_PORT=your_port

# The username for accessing the application
IMAGEFLOW_USERNAME=your_username

# The hashed password for accessing the application
IMAGEFLOW_PASSWORD=your_hashed_password

# The secret key used for session management
IMAGEFLOW_SESSION_SECRET=your_session_secret

# The maximum size of a single file upload in bytes
IMAGEFLOW_MAX_FILE_SIZE=your_max_file_size

# The maximum total size of all uploaded files in bytes
IMAGEFLOW_MAX_TOTAL_FILE_SIZE=your_max_total_file_size

# The maximum number of files allowed per upload
IMAGEFLOW_MAX_FILES=your_max_files

# The allowed file extensions for uploads
IMAGEFLOW_ALLOWED_EXTENSIONS=your_allowed_extensions

# The maximum number of login attempts allowed
IMAGEFLOW_LOGIN_RATE_LIMIT_MAX=your_login_rate_limit_max

# The time window for login rate limiting in milliseconds
IMAGEFLOW_LOGIN_RATE_LIMIT_WINDOW_MS=your_login_rate_limit_window_ms

# The expiration time for download tokens in milliseconds
IMAGEFLOW_DOWNLOAD_TOKEN_EXPIRY=your_download_token_expiry